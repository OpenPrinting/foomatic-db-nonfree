<driver id="driver/dj505j">
  <name>dj505j</name>
  <url>http://www.openprinting.org/download/japanese_printers/</url>
  <execution>
    <ghostscript/>
    <prototype>gs -q -dBATCH -dPARANOIDSAFER -dQUIET -dNOPAUSE -sDEVICE=dj505j%A%Z -sOutputFile=- -</prototype>
    </execution>
  <comments>
    <en>
      Special japanese printer driver.
      </en>
    </comments>
  <printers>
    <printer>
      <id>printer/HP-DeskJet_505J_Plus</id>
      </printer>
    </printers>
  </driver>
