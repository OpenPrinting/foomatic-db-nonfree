<driver id="driver/xes">
 <name>xes</name>
 <url>http://www.cs.wisc.edu/~ghost/doc/printer.htm</url>
 <execution>
  <ghostscript />
  <prototype>gs -q -dBATCH -dPARANOIDSAFER -dQUIET -dNOPAUSE -sDEVICE=xes%A%Z -sOutputFile=- -</prototype>
 </execution>
 
 <printers>
  <printer>
   <id>printer/Xerox-2700_XES</id><!-- Xerox 2700 XES -->
  </printer>
  <printer>
   <id>printer/Xerox-3700_XES</id><!-- Xerox 3700 XES -->
  </printer>
  <printer>
   <id>printer/Xerox-4045_XES</id><!-- Xerox 4045 XES -->
  </printer>
 </printers>
</driver>