<driver id="driver/md1xMono">
  <name>md1xMono</name>
  <url>http://www.openprinting.org/download/japanese_printers/</url>
  <execution>
    <ghostscript/>
    <prototype>gs -q -dBATCH -dPARANOIDSAFER -dQUIET -dNOPAUSE -sDEVICE=md1xMono%A%Z -sOutputFile=- -</prototype>
    </execution>
  <comments>
    <en>
      Special japanese printer driver.
      </en>
    </comments>
  <printers>
    <printer>
      <id>printer/Alps-MD-1300</id> <!-- Alps MD-1300 -->
      </printer>
     <printer>
      <id>printer/Alps-MD-1500</id>
      </printer>
    </printers>
  </driver>
