<driver id="driver/npdl">
  <name>npdl</name>
  <url>http://www.openprinting.org/download/japanese_printers/</url>
  <execution>
    <ghostscript/>
    <prototype>gs -q -dBATCH -dPARANOIDSAFER -dQUIET -dNOPAUSE -sDEVICE=npdl%A%Z -sOutputFile=- -</prototype>
    </execution>
  <comments>
    <en>
      Special japanese printer driver.
      </en>
    </comments>
  <printers>
    <printer>
      <id>printer/NEC-MultiWriter</id>
      </printer>
     <printer>
      <id>printer/NEC-PC-PR1000</id>
      </printer>
     <printer>
      <id>printer/NEC-PC-PR2000</id>
      </printer>
    </printers>
  </driver>
