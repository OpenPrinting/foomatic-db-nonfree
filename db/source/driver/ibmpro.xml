<driver id="driver/ibmpro">
 <name>ibmpro</name>
 <url>http://www.cs.wisc.edu/~ghost/doc/printer.htm</url>
 <execution>
  <ghostscript />
  <prototype>gs -q -dBATCH -dPARANOIDSAFER -dQUIET -dNOPAUSE -sDEVICE=ibmpro%A%Z -sOutputFile=- -</prototype>
 </execution>
 
 <printers>
  <printer>
   <id>printer/Generic-IBM-Compatible_Dot_Matrix_Printer</id>
  </printer>
  <printer>
   <id>printer/IBM-ProPrinterII</id><!-- IBM ProPrinterII -->
  </printer>
  <printer>
   <id>printer/Okidata-Microline_192plus</id><!-- Okidata Microline 192+ -->
  </printer>
  <printer>
   <id>printer/Panasonic-KX-P2150</id><!-- Panasonic KX-P2150 -->
  </printer>
 </printers>
</driver>