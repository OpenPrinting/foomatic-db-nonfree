<driver id="driver/deskjet">
 <name>deskjet</name>
 <url>http://www.cs.wisc.edu/~ghost/doc/printer.htm</url>
 <execution>
  <ghostscript />
  <prototype>gs -q -dBATCH -dPARANOIDSAFER -dQUIET -dNOPAUSE -sDEVICE=deskjet%A%Z -sOutputFile=- -</prototype>
 </execution>
 
 <printers>
  <printer>
   <id>printer/HP-DeskJet</id><!-- HP DeskJet -->
  </printer>
  <printer>
   <id>printer/HP-DeskJet_200</id>
  </printer>
  <printer>
   <id>printer/HP-DeskJet_340C</id><!-- HP DeskJet 340C -->
  </printer>
  <printer>
   <id>printer/HP-DeskJet_Plus</id><!-- HP DeskJet Plus -->
  </printer>
  <printer>
   <id>printer/HP-OfficeJet_R45</id><!-- HP OfficeJet R45 -->
  </printer>
 </printers>
</driver>