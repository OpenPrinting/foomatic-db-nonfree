<driver id="driver/lq850">
 <name>lq850</name>
 <url>http://www.cs.wisc.edu/~ghost/doc/printer.htm</url>
 <execution>
  <ghostscript />
  <prototype>gs -q -dBATCH -dPARANOIDSAFER -dQUIET -dNOPAUSE -sDEVICE=lq850%A%Z -sOutputFile=- -</prototype>
 </execution>
 
 <printers>
  <printer>
   <id>printer/Generic-ESC_P_Dot_Matrix_Printer</id>
  </printer>
  <printer>
   <id>printer/Brother-HJ-400</id><!-- Brother HJ-400 -->
  </printer>
  <printer>
   <id>printer/Canon-BJ-300</id><!-- Canon BJ-300 -->
  </printer>
  <printer>
   <id>printer/Epson-LQ-850</id><!-- Epson LQ-850 -->
  </printer>
 </printers>
</driver>