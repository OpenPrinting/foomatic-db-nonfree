<driver id="driver/mj8000c">
  <name>mj8000c</name>
  <url>http://www.openprinting.org/download/japanese_printers/</url>
  <execution>
    <ghostscript/>
    <prototype>gs -q -dBATCH -dPARANOIDSAFER -dQUIET -dNOPAUSE -sDEVICE=mj8000c%A%Z -sOutputFile=- -</prototype>
    </execution>
  <comments>
    <en>
      Special japanese printer driver.
      </en>
    </comments>
  <printers>
    <printer>
      <id>printer/Epson-Stylus_Color_3000</id>
      </printer>
    </printers>
  </driver>
