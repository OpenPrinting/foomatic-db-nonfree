<driver id="driver/mj500c">
  <name>mj500c</name>
  <url>http://www.openprinting.org/download/japanese_printers/</url>
  <execution>
    <ghostscript/>
    <prototype>gs -q -dBATCH -dPARANOIDSAFER -dQUIET -dNOPAUSE -sDEVICE=mj500c%A%Z -sOutputFile=- -</prototype>
    </execution>
  <comments>
    <en>
      Special japanese printer driver.
      </en>
    </comments>
  <printers>
    <printer>
      <id>printer/Epson-Stylus_Color_1500</id>
      </printer>
     <printer>
      <id>printer/Epson-Stylus_Color_200</id>
      </printer>
     <printer>
      <id>printer/Epson-Stylus_Color_IIs</id>
      </printer>
    </printers>
  </driver>
