<driver id="driver/pjxl300">
 <name>pjxl300</name>
 <url>http://www.ghostscript.com/doc/gnu/7.05/Devices.htm#HP_color_inkjet</url>
 <execution>
  <ghostscript />
  <prototype>gs -q -dBATCH -dPARANOIDSAFER -dQUIET -dNOPAUSE -r300x300 -sDEVICE=pjxl300%A%Z -sOutputFile=- -</prototype>
 </execution>
 
 <printers>
  <printer>
   <id>printer/HP-PaintJet_XL300</id><!-- HP PaintJet XL300 -->
  </printer>
  <printer>
   <id>printer/HP-DeskJet_1200C</id><!-- HP DeskJet 1200C -->
  </printer>
  <printer>
   <id>printer/HP-PhotoSmart_P100</id>
  </printer>
 </printers>
</driver>