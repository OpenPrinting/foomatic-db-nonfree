<driver id="driver/lbp8">
 <name>lbp8</name>
 <url>http://www.cs.wisc.edu/~ghost/doc/printer.htm</url>
 <execution>
  <ghostscript />
  <prototype>gs -q -dBATCH -dPARANOIDSAFER -dQUIET -dNOPAUSE -sDEVICE=lbp8%A%Z -sOutputFile=- -</prototype>
 </execution>
 
 <printers>
  <printer>
   <id>printer/Canon-LBP-4plus</id><!-- Canon LBP-4+ -->
  </printer>
  <printer>
   <id>printer/Canon-LBP-4U</id><!-- Canon LBP-4U -->
  </printer>
  <printer>
   <id>printer/Canon-LBP-8A1</id><!-- Canon LBP-8A1 -->
   <comments>
    <en>Special-purpose gs driver.  Bound to work.</en>
   </comments>
  </printer>
 </printers>
</driver>