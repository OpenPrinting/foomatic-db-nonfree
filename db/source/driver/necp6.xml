<driver id="driver/necp6">
 <name>necp6</name>
 <url>http://www.cs.wisc.edu/~ghost/doc/printer.htm</url>
 <execution>
  <ghostscript />
  <prototype>gs -q -dBATCH -dPARANOIDSAFER -dQUIET -dNOPAUSE -sDEVICE=necp6%A%Z -sOutputFile=- -</prototype>
 </execution>
 
 <printers>
  <printer>
   <id>printer/NEC-PinWriter_P6</id><!-- NEC PinWriter P6 -->
  </printer>
  <printer>
   <id>printer/NEC-PinWriter_P6_plus</id><!-- NEC PinWriter P6 plus -->
  </printer>
  <printer>
   <id>printer/NEC-PinWriter_P7</id><!-- NEC PinWriter P7 -->
  </printer>
  <printer>
   <id>printer/NEC-PinWriter_P7_plus</id><!-- NEC PinWriter P7 plus -->
  </printer>
  <printer>
   <id>printer/NEC-PinWriter_P60</id><!-- NEC PinWriter P60 -->
  </printer>
  <printer>
   <id>printer/NEC-PinWriter_P70</id><!-- NEC PinWriter P70 -->
  </printer>
  <printer>
   <id>printer/NEC-Pinwriter_P20</id><!-- NEC Pinwriter P20 -->
  </printer>
  <printer>
   <id>printer/Star-LC24-100</id><!-- Star LC24-100 -->
  </printer>
 </printers>
</driver>