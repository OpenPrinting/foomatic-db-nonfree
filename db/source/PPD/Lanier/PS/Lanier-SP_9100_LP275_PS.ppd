	<<
		/PatternType 1
		/PaintType 1
		/TilingType 3
		/BBox [ 0 0 1 1 ]
		/XStep 1 
		/YStep 1 
		/PaintProc {
			begin
				RPS_WM_color
				8 8 true [ 8 0 0 -8 0 8 ]
				<88002200 88002200> imagemask
			end
		}
	>> [ 1 0 0 -1 0 1 ] makepattern
	setpattern }
   bind def
