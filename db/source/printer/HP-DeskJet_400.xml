  <model>DeskJet 400</model>
  <mechanism>
    <inkjet/>
    <color/>
    <resolution>
      <dpi>
        <x>300</x>
        <y>300</y>
      </dpi>
    </resolution>
    <consumables>
      <comments>
        <en>Cartridge (HP 51625A for color, HP 51626A for B/W)</en>
      </comments>
