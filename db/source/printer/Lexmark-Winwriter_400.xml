<printer id="printer/Lexmark-Winwriter_400">
  <make>Lexmark</make>
  <model>Winwriter 400</model>
  <mechanism>
    <laser/><!--not "color"-->
    <resolution>
      <dpi>
        <x>600</x>
        <y>300</y>
      </dpi>
    </resolution>
    <consumables>
      <comments>
        <en>cartridge</en>
      </comments>
    <!--one or more "partno" elements.--></consumables>
  </mechanism><!--no "url"-->
  <lang>
    <pcl level="4"/><!--no pjl-->
    <text>
      <charset>us-ascii</charset>
    </text>
  </lang>
  <autodetect>
  <!--no known parport probe information--></autodetect>
  <functionality>D</functionality>
  <driver>ljetplus</driver>
  <!--not "unverified"--><!--no "contrib_url"-->
  <comments>
    <en>Emulates HP Laserjet II under any OS, but needs Windows driver to get 600x300 dpi.</en>
  </comments>
</printer>
