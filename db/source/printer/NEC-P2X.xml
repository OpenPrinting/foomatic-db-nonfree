<printer id="printer/NEC-P2X">
  <make>NEC</make>
  <model>P2X</model>
  <mechanism>
    <dotmatrix/><!--not "color"-->
    <resolution>
      <dpi>
        <x>360</x>
        <y>360</y>
      </dpi>
    </resolution>
    <consumables>
      <comments>
        <en>ribbon</en>
      </comments>
    <!--one or more "partno" elements.--></consumables>
  </mechanism>
  <url>http://cssweb.nectech.com/Pwriter/p32/spec1.htm</url>
  <lang>
    <escp/><!--no pjl-->
    <text>
      <charset>us-ascii</charset>
    </text>
  </lang>
  <autodetect>
  <!--no known parport probe information--></autodetect>
  <functionality>A</functionality>
  <driver>necp2x.upp</driver>
  <!--not "unverified"--><!--no "contrib_url"-->
  <comments>
    <en>uniprint parameter file nxp2x.upp</en>
  </comments>
</printer>
