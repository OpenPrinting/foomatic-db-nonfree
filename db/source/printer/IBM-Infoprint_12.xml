<printer id="printer/IBM-Infoprint_12">
  <make>IBM</make>
  <model>Infoprint 12</model>
  <mechanism>
    <laser/><!--not "color"-->
    <resolution>
      <dpi>
        <x>1200</x>
        <y>1200</y>
      </dpi>
    </resolution>
    <consumables>
      <comments>
        <en>6000p. Toner cartrige. (~$150 1/1/2001)</en>
      </comments>
    <!--one or more "partno" elements.--></consumables>
  </mechanism>
  <url>http://www.printers.ibm.com/R5PSC.NSF/Web/ip12Home</url>
  <lang>
    <pcl level="6"/>
    <pjl/>
    <text>
      <charset>us-ascii</charset>
    </text>
  </lang>
  <autodetect>
    <parallel>
      <commandset>PCL5E,PCL6</commandset>
      <manufacturer>IBM</manufacturer>
      <model>Infoprint</model>
    </parallel>
  </autodetect>
  <functionality>A</functionality>
  <driver>pxlmono</driver>
  <!--not "unverified"-->
  <contrib_url>http://www.cs.wisc.edu/~ghost</contrib_url>
  <comments>
    <en>Supports PCL6 &amp;amp; PCL5e.  Optional Postscript level 3 and fast ethernet cards  cost about $200 each (1/1/2001).  Comes with 4MB memory expandable to 68MB.</en>
  </comments>
</printer>
