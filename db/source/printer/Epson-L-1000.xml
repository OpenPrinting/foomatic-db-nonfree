<printer id="printer/Epson-L-1000">
  <make>Epson</make>
  <model>L-1000</model>
  <mechanism>
    <dotmatrix/><!--not "color"-->
    <resolution>
      <dpi>
        <x>360</x>
        <y>180</y>
      </dpi>
    </resolution>
    <consumables>
      <comments>
        <en>fabric ribbon; can be reinked with third party tool</en>
      </comments>
    <!--one or more "partno" elements.--></consumables>
  </mechanism><!--no "url"-->
  <lang>
    <escp/><!--no pjl-->
    <text>
      <charset>us-ascii</charset>
    </text>
  </lang>
  <autodetect>
  <!--no known parport probe information--></autodetect>
  <functionality>A</functionality>
  <driver>epson</driver>
  <!--not "unverified"--><!--no "contrib_url"-->
  <comments>
    <en>Used Redhat 6.1 Printtool.  Told it I had an Epson 24-pin.
Works like a charm with &quot;Type A&quot; resolutions.</en>
  </comments>
</printer>
