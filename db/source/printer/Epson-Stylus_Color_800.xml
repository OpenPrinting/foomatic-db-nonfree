<printer id="printer/Epson-Stylus_Color_800">
  <make>Epson</make>
  <model>Stylus Color 800</model>
  <mechanism>
    <inkjet/>
    <color/>
    <resolution>
      <dpi>
        <x>1440</x>
        <y>720</y>
      </dpi>
    </resolution>
    <consumables>
      <comments>
        <en>color and black cartridge</en>
      </comments>
    <!--one or more "partno" elements.--></consumables>
  </mechanism>
  <url>http://support.epson.com/hardware/printer/inkjet/sc800_/index.html</url>
  <lang>
    <escp/><!--no pjl-->
    <text>
      <charset>us-ascii</charset>
    </text>
  </lang>
  <autodetect>
    <parallel>
      <commandset>ESCPL2,PRPXL24,BDC</commandset>
      <manufacturer>EPSON</manufacturer>
      <model>Stylus COLOR 800</model>
    </parallel>
  </autodetect>
  <functionality>A</functionality>
  <driver>gutenprint</driver><!--verified--><!--no "contrib_url"-->
  <comments>
    <en>Probably works best with the stp driver.</en>
  </comments>
</printer>
