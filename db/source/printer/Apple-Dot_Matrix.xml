<printer id="printer/Apple-Dot_Matrix">
  <make>Apple</make>
  <model>Dot Matrix</model>
  <mechanism>
    <dotmatrix/><!--not "color"-->
    <resolution>
      <dpi>
        <x>120</x>
        <y>72</y>
      </dpi>
    </resolution>
    <consumables>
    <!--no comments--><!--one or more "partno" elements.--></consumables>
  </mechanism><!--no "url"-->
  <lang>
    <proprietary/><!--no pjl-->
    <text>
      <charset>us-ascii</charset>
    </text>
  </lang>
  <autodetect>
  <!--no known parport probe information--></autodetect>
  <functionality>A</functionality>

  <driver>appledmp</driver><!--verified--><!--no "contrib_url"--><!--no comments!--></printer>
