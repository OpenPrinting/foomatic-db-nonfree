<printer id="printer/Lexmark-Valuewriter_300">
  <make>Lexmark</make>
  <model>Valuewriter 300</model>
  <mechanism>
    <laser/><!--not "color"-->
    <resolution>
      <dpi>
        <x>300</x>
        <y>300</y>
      </dpi>
    </resolution>
    <consumables>
      <comments>
        <en>cartridge</en>
      </comments>
    <!--one or more "partno" elements.--></consumables>
  </mechanism><!--no "url"-->
  <lang>
    <pcl level="4"/><!--no pjl-->
    <text>
      <charset>us-ascii</charset>
    </text>
  </lang>
  <autodetect>
  <!--no known parport probe information--></autodetect>
  <functionality>A</functionality>
  <driver>ljet2p</driver>
  <!--not "unverified"--><!--no "contrib_url"-->
  <comments>
    <en>Reported to work well as a laserjet II on Usenet.
 </en>
  </comments>
</printer>
