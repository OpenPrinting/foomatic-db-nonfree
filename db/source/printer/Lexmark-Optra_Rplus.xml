<printer id="printer/Lexmark-Optra_Rplus">
  <make>Lexmark</make>
  <model>Optra R+</model>
  <mechanism>
    <laser/><!--not "color"-->
    <resolution>
      <dpi>
        <x>1200</x>
        <y>1200</y>
      </dpi>
    </resolution>
    <consumables>
      <comments>
        <en>Cartridge. Lasts about 14000 pages. Costs about 150GBP</en>
      </comments>
    <!--one or more "partno" elements.--></consumables>
  </mechanism><!--no "url"-->
  <lang>
    <postscript level="2">
    <!--unknown ppd filename "ppd"--></postscript>
    <pjl/>
    <text>
      <charset>us-ascii</charset>
    </text>
  </lang>
  <autodetect>
    <parallel>
      <manufacturer>Lexmark International</manufacturer>
      <model>Lexmark Optra LaserPrinter</model>
      <description>Lexmark Optra LaserPrinter</description>
      <commandset>PCL 5 Emulation, PostScript Level 2 Emulation, NPAP, PJL</commandset>
    </parallel>
  </autodetect>
  <functionality>A</functionality>
  <driver>Postscript</driver>
  <!--not "unverified"--><!--no "contrib_url"-->
  <comments>
    <en>Also speaks PCL level 5.
For 1200DPI use Postscript Level 2.
Std 2MB RAM adequate for 600DPI
Postscript, RAM is expandable to
64MB using 72pin SIMMs.</en>
  </comments>
</printer>
