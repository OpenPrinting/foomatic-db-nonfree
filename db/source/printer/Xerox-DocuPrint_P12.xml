<printer id="printer/Xerox-DocuPrint_P12">
  <make>Xerox</make>
  <model>DocuPrint P12</model>
  <mechanism>
    <laser/><!--not "color"-->
    <resolution>
      <dpi>
        <x>600</x>
        <y>600</y>
      </dpi>
    </resolution>
    <consumables>
    <!--no comments--><!--one or more "partno" elements.--></consumables>
  </mechanism>
  <url>http://www.xerox.com/go/xrx/products/overview.jsp?id=P12&amp;cat=%2fEquipment%2fPrinting%2fDesktop</url>
  <lang>
    <pcl level="4"/><!--no pjl-->
    <text>
      <charset>us-ascii</charset>
    </text>
  </lang>
  <autodetect>
    <parallel>
      <commandset>PrintGear,PCL,PJL</commandset>
      <description>Xerox DocuPrint P12</description>
      <manufacturer>XEROX</manufacturer>
      <model>DocuPrint P12</model>
    </parallel>
  </autodetect>
  <functionality>B</functionality>
  <driver>ljet2p</driver>
  <!--not "unverified"--><!--no "contrib_url"-->
  <comments>
    <en>Another &quot;PrintGear&quot; printer; PCL support with Ghostscript driver only goes up to 300x300 dpi.</en>
  </comments>
</printer>
