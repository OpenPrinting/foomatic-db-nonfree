<printer id="printer/Lexmark-3000">
  <make>Lexmark</make>
  <model>3000</model>
  <mechanism>
    <inkjet/>
    <color/>
    <resolution>
      <dpi>
        <x>600</x>
        <y>300</y>
      </dpi>
    </resolution>
    <consumables>
      <comments>
        <en>dual cartridge</en>
      </comments>
    <!--one or more "partno" elements.--></consumables>
  </mechanism>
  <url>http://www.lexmark.com/printers/inkjet/3000/</url>
  <lang>
    <pcl level="3"/><!--no pjl-->
    <text>
      <charset>us-ascii</charset>
    </text>
  </lang>
  <autodetect>
    <parallel>
      <commandset>LEXWPS,PCL3</commandset>
      <description>Lexmark 3000 Color Jetprinter</description>
      <manufacturer>Lexmark</manufacturer>
      <model>Lexmark 3000 Color Jetprinter</model>
    </parallel>
  </autodetect>
  <functionality>B</functionality>
  <driver>pcl3</driver>
  <!--not "unverified"--><!--no "contrib_url"-->
  <comments>
    <en>600x300 DPI printing mode and
photo cartridge not supported.
Use 300x300 DPI.

